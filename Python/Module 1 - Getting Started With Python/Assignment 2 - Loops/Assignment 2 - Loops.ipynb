   "source": [
    "#Create a list that is having 10,23,4,26,4,75,24,54 values and with the help of while loop,\n",
    "#fetch the even numbers and print the numbers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "d8777fff",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "10\n",
      "4\n",
      "26\n",
      "4\n",
      "24\n",
      "54\n"
     ]
    }
   ],
   "source": [
    "l = [10,23,4,26,4,75,24,54]\n",
    "\n",
    "i = 0\n",
    "\n",
    "while i < len(l):\n",
    "    if l[i] % 2 == 0:\n",
    "        print(l[i])\n",
    "    i += 1"
   ]
  }
